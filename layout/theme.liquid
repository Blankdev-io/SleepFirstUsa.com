                        {{- current_tags | join: ' + ' -}}
                      </a>
                    </li>
                  {%- else -%}
                    <li class="inline-block">
                      <a class="text-primary hover:underline" href="{{ blog.url }}" aria-current="page">
                        {{- blog.title -}}
                      </a>
                    </li>
                  {%- endif -%}
                {%- when 'article' -%}
                  <li class="inline-block mr-1">
                    <span class="hover:text-primary hover:underline">{{ blog.title | link_to: blog.url }}</span>
                    <span class="inline-block -rotate-90 pt-1">{% render 'icons', icon: 'blue-down' %}</span>
